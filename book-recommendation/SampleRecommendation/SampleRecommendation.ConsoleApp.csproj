<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ML" Version="1.7.0" />
    <PackageReference Include="Microsoft.ML.Recommender" Version="0.19.0" />
  </ItemGroup>
  <ItemGroup Label="SampleRecommendation">
    <None Include="SampleRecommendation.consumption.cs">
      <DependentUpon>SampleRecommendation.mbconfig</DependentUpon>
    </None>
    <None Include="SampleRecommendation.training.cs">
      <DependentUpon>SampleRecommendation.mbconfig</DependentUpon>
    </None>
    <None Include="SampleRecommendation.zip">
      <DependentUpon>SampleRecommendation.mbconfig</DependentUpon>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>